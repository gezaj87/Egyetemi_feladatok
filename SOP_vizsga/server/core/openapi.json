            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "email",
                  "password"
                ],
                "properties": {
                  "email": {
                    "type": "string",
                    "description": "email",
                    "example": "email@email.hu"
                  },
                  "password": {
                    "type": "string",
                    "description": "Password",
                    "example": "testPassword123"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User login",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "success",
                    "message",
                    "token"
                  ],
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "description": "Success",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "description": "Message",
                      "example": "User logged in"
                    },
                    "token": {
                      "type": "string",
                      "description": "Token",
                      "example": 1234567890
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
