  "paths": {
    "/auth": {
      "post": {
        "summary": "User authentication",
        "description": "User authentication",
        "operationId": "auth",
        "tags": [
          "auth"
        ],
        "requestBody": {
          "description": "User authentication",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "token"
                ],
                "properties": {
                  "token": {
                    "type": "string",
                    "description": "User token",
                    "example": 1234567890
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User authentication",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "success",
                    "message"
                  ],
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "description": "Success",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "description": "Message",
                      "example": "User authenticated"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/login": {
