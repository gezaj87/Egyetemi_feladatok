    "/cars": {
      "post": {
        "summary": "Cars",
        "description": "Cars",
        "operationId": "cars",
        "tags": [
          "cars"
        ],
        "requestBody": {
          "description": "Cars",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "token",
                  "avaiableOnly"
                ],
                "properties": {
                  "token": {
                    "type": "string",
                    "description": "User token",
                    "example": 1234567890
                  },
                  "avaiableOnly": {
                    "type": "boolean",
                    "description": "Avaiable only",
                    "example": true
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Cars",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "success",
                    "message",
                    "cars"
                  ],
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "description": "Success",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "description": "Message",
                      "example": "Cars"
                    },
                    "cars": {
                      "type": "array",
                      "description": "Cars",
                      "items": {
                        "type": "object",
                        "required": [
                          "plate",
                          "year",
                          "capacity",
                          "price",
                          "image",
                          "avaiable",
                          "brand"
                        ],
                        "properties": {
                          "plate": {
                            "type": "string",
                            "description": "Plate",
                            "example": "ABC-123"
                          },
                          "year": {
                            "type": "integer",
                            "description": "Year",
                            "example": 2010
                          },
                          "capacity": {
                            "type": "integer",
                            "description": "Capacity",
                            "example": 5
                          },
                          "price": {
                            "type": "integer",
                            "description": "Price",
                            "example": 500
                          },
                          "image": {
                            "type": "string",
                            "description": "Image",
                            "example": "image.jpg"
                          },
                          "avaiable": {
                            "type": "integer",
                            "description": "Avaiable",
                            "example": 1
                          },
                          "brand": {
                            "type": "string",
                            "description": "Brand",
                            "example": "BMW"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/pay": {
      "post": {
        "summary": "Pay",
        "description": "Pay",
        "operationId": "pay",
        "tags": [
          "pay"
        ],
        "requestBody": {
          "description": "Pay",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "token",
                  "name",
                  "cardNumber",
                  "expDate",
                  "cvc",
                  "price",
                  "plate"
                ],
                "properties": {
                  "token": {
                    "type": "string",
                    "description": "User token",
                    "example": 1234567890
                  },
                  "name": {
                    "type": "string",
                    "description": "Name",
                    "example": "John Doe"
                  },
                  "cardNumber": {
                    "type": "string",
                    "description": "Card number",
                    "example": "1234 5678 9012 3456"
                  },
                  "expDate": {
                    "type": "string",
                    "description": "Expiration date",
                    "example": "12/24"
                  },
                  "cvc": {
                    "type": "integer",
                    "description": "CVC",
                    "example": 123
                  },
                  "price": {
                    "type": "integer",
                    "description": "Price",
                    "example": 500
                  },
                  "plate": {
                    "type": "string",
                    "description": "Plate",
                    "example": "ABC-123"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Pay",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "success",
                    "message",
                    "payment_id"
                  ],
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "description": "Success",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "description": "Message",
                      "example": "Payment successful"
                    },
                    "payment_id": {
                      "type": "string",
                      "description": "Payment id",
                      "example": 1234567890
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
