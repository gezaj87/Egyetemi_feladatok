                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "User data update",
        "description": "User data update",
        "operationId": "user_put",
        "tags": [
          "user"
        ],
        "requestBody": {
          "description": "User data update",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "token",
                  "email",
                  "name",
                  "address",
                  "isPasswordChanged",
                  "oldPassword",
                  "newPassword1",
                  "newPassword2"
                ],
                "properties": {
                  "token": {
                    "type": "string",
                    "description": "User token",
                    "example": 1234567890
                  },
                  "email": {
                    "type": "string",
                    "description": "email",
                    "example": "email@email.hu"
                  },
                  "name": {
                    "type": "string",
                    "description": "Name",
                    "example": "John Doe"
                  },
                  "address": {
                    "type": "string",
                    "description": "Address",
                    "example": "1211 Budapest, Kossuth Lajos utca 1."
                  },
                  "isPasswordChanged": {
                    "type": "boolean",
                    "description": "Is password changed",
                    "example": true
                  },
                  "oldPassword": {
                    "type": "string",
                    "description": "Old password",
                    "example": "testPassword123"
                  },
                  "newPassword1": {
                    "type": "string",
                    "description": "New password 1",
                    "example": "testPassword123"
                  },
                  "newPassword2": {
                    "type": "string",
                    "description": "New password 2",
                    "example": "testPassword123"
                  }
                }
              }
